{
  "name": "express-locallibrary",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:server": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "serverstart": "DEBUG=express-locallibrary:* npm run devstart",
    "dev": "npx nodemon",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "start": "nodemon dist/index.js",
    "server:start": "DEBUG=express-locallibrary:* npm run build && npm run start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm -- -d ./src/config/data-source.ts migration:generate ./src/migration/$npm_config_name",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/config/data-source.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- -d ./src/config/data-source.ts",
    "typeorm:show": "npm run typeorm migration:show -- -d ./src/config/data-source.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.9.0",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
